  <li class="active">所属マスタ</li>
</ol>

<!-- <h1>工程マスタ</h1> -->

<table id="shozokumaster" class="table table-striped table-bordered table-hover" cellspacing="0">

  <thead>
  <tr>
    <th>所属コード</th>
    <th>所属名</th>
  </tr>
  </thead>

  <tbody>
  <% @shozokus.each do |shozoku| %>
      <tr id="shozoku_<%= shozoku.id %>">
      <td><%= shozoku.所属コード %></td>
        <td><%= shozoku.所属名 %></td>
      </tr>
  <% end %>
  </tbody>
</table>